@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';

.aui-searchbar-input {
    height:80rpx !important;
    line-height: 80rpx !important;
    border-radius: 0 !important;
}

.aui-searchbar-input input {
    height:80rpx !important;
    text-align: left;
}

.aui-searchbar-cancel image {
    width:35rpx !important;
    margin-right: 7rpx;
}

.aui-list-item-media {
    width:110rpx !important;
}

.pic {
    height: 80rpx;
    width: 80rpx;
    border-radius: 50%;
    overflow: hidden;
}

.pic image {
    width: 100%;
    height: 100%;
    object-fit: cover;
}
.care{
  width: 200rpx;
  height: 68rpx;
  line-height: 68rpx;
  border-radius: 10rpx;
}
.fabu{
  color: red;
  font-size: 26rpx;
}
.box{
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
}
