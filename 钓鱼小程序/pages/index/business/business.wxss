@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';

page {
    background-color: #f5f6fa;
}

.shangjia .aui-list-item-media {
    width: 110rpx !important;
    padding-right: 20rpx !important;
}

.shangjia .shop-pic {
    width: 90rpx;
    height: 90rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.shangjia .shop-pic image {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.shangjia .aui-col-xs-4 {
    width: 32%;
    margin-right: 2%;
}

.shangjia .aui-col-xs-4:nth-of-type(3) {
    margin-right: 0 !important;
}

.aui-col-xs-4 .pic {
    height: 210rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.aui-col-xs-4 .pic image {
    width: 100%;
    height: 100%;
    object-fit: cover;
}
