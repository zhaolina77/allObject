@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';

page {
    background-color: #f8f9fb;
}
.lay {
    background-color: rgba(0, 0, 0, 0.5);
    position: absolute;
    top: 0;
    left: 0;
    height:440rpx;
    width: 100%;
}
.aui-bar image {
    width: 40rpx;
    vertical-align: middle;
}

.aui-bar .aui-title {
    right: 80rpx !important;
    display: flex;
    align-items: center;
    left: 80rpx !important;
}

.top-pic {
    height:260rpx;
    overflow: hidden;
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    z-index: 10;
}

.top-pic image {
    width: 100%;
    height: 100%;
    object-fit: cover;
}
.void video{
  width: 100%;
  height: 100%;
}