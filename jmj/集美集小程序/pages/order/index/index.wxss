@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';
/* .aui-tab{position: fixed;top: 0;left: 0;z-index: 999;} */
page{background-color: #F5F6FA !important;}
scroll-view {
    height: 90rpx;
    line-height: 90rpx;
    position: fixed;top: 0;left: 0;z-index: 999;
    background-color: white;
}

scroll-view .swiper-slide {
    margin-left: 30rpx;
    margin-right: 30rpx;
}

scroll-view .swiper-slide:last-child {
    margin-right: 120rpx;
}

scroll-view .swiper-slide.on text {
    color: #222 !important;
    font-weight: 500;
}

.aui-list-item-media {
    width: auto !important;
}

.shop-pic {
    width:160rpx;
    height:160rpx;
    overflow: hidden;
}

.shop-pic image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}

.aui-btn {
  width: 150rpx !important;
    margin-left:20rpx;
    height:65rpx !important;
    line-height:65rpx !important;
}

.aui-btn-tuan{width: 36rpx !important;height: 36rpx !important;line-height: 36rpx;border-radius: 6rpx;}
.aui-list:last-child{margin-bottom: 20rpx;}
