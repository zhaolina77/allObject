@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';

page {
    background-color: #f5f6fa !important;
}

.aui-list .aui-list-item {
    padding-left: 0;
    margin-left:30rpx;
}

.aui-list-item-media {
    width:130rpx !important;
}

.aui-list-item-media img {
    width: 100%;
    height:100rpx;
    object-fit: cover;
    border-radius:80rpx;
}

.aui-list .aui-list-item:last-child .aui-list-item-inner {
    border-bottom: none !important;
}

.aui-list-item-text {
    height:100rpx;
    line-height:100rpx;
}

.aui-badge {
    position: initial !important;
}
