@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';
@import '../../../wxParse/wxParse.wxss';
page{background-color: #fff !important;}
.detail .banner{
  height:500rpx;
  position: relative;
  background-color: #F5F6FA;
}
.detail .banner swiper{
  height:500rpx;
  position: relative;
  
}
.detail .banner swiper-item image{
  width: 100%;
  height: 100%;
}

.map{width: 100%;height: 400rpx;overflow: hidden;}
.map image{width: 100%;height: 100%;}
.slide-pic{overflow: inherit;  }

.huxing  .slide{margin-bottom: 16rpx;}
.huxing  .slide .slide-pic{margin-right: 20rpx;} 
.huxing .slide .slide-pic image{width:310rpx;height:240rpx;border-radius: 10rpx;} 
.address{width: 32rpx;height: 32rpx;}

.footer button{padding: 0 !important;}
.footer button, .aui-btn{height: unset !important}


.aui-col-xs-2{width: 15%;margin-right: 2%;}
.aui-col-xs-2:nth-of-type(6n){margin-right: 0 !important;}
.aui-col-xs-2 .icon{width: 50rpx;height: 50rpx;margin: 0 auto;}
.aui-col-xs-2 .icon image{width: 100%;height: 100%;}
