<!--pages/my/address/index.wxml-->

<view class="aui-bg-white aui-margin-t-15 aui-padded-15 aui-padded-t-0" wx:if="{{list.length>0}}">
  <view class="list" wx:for="{{list}}">
    
    <view class="flex" data-item="{{item}}"  bindtap="gotaddress">

      <text class="aui-font-size-13 aui-text-blacker flex1">{{item.province}}{{item.city}}{{item.district}} {{item.address}}</text>

     

    </view>
  </view>


  

</view>
<image src="/images/noaddress.png" mode="aspectFit" wx:else></image>

