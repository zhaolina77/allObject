/**index.wxss**/
@import '/pages/aui.wxss';
@import '/pages/common.wxss';
page{
  position: relative;
  height: 100vh;
}
.container{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 20rpx;
  height: auto;
}
.pics {
  width: 100%;
  height: 1000rpx;
}
.share{
  width: 100%;
  height: 80rpx;
  color: #fff;
  background: rgb(105, 163, 250);
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;

}

.imgM{
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.3);
  position: fixed;
  top: 0;
  left: 0;
}
.imgBox{
  width: 300px;
  height: 540px;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.imgBox canvas{
  width: 300px;
  height: 500px;
  position: fixed;
}
.imgBox .btn{
  width: 100%;
  height: 40px;
  background: rgb(4, 230, 124);
  color: #fff;
  text-align: center;
  line-height: 40px;
  position: absolute;
  bottom: 0;
}

