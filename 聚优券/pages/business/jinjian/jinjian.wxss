@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';
page{background-color: #fff !important;}

.aui-iconfont {
    font-size: 12rpx;
}

.inner {
    margin-left: 60rpx;
}

.aui-border-b {
    border-color: #eee !important;
}

.li {
    position: relative;
}

.li:after {
    content: '';
    position: absolute;
    height: 100%;
    width: 1px;
    background-color: #ccc;
    top:34%;
    left:25rpx;
}

.li:last-child:after {
    display: none;
}
.jd1 image{width: 30rpx;height: 30rpx;}
.jd1 {
    position: absolute;
    left: 10rpx;
    top: 34%;
    margin-top: -8rpx;
    width: 30rpx;
    height:30rpx;
    line-height:30rpx;
    border-radius:15rpx;
    z-index: 9;
}
