.aui-list-item-media {
    width:110rpx !important;
}
.head-pic{width: 64rpx !important;height: 64rpx !important;}
.pic {
    height: 80rpx;
    width: 80rpx;
    border-radius: 50%;
    overflow: hidden;
}

.pic image {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.pl li:last-child .nr {
    border-bottom: none !important;
}

.aui-searchbar-input {
    height: 80rpx !important;
    line-height:80rpx !important;
    border-radius: 0 !important;
}

.aui-searchbar-input input {
    height:80rpx !important;
}
