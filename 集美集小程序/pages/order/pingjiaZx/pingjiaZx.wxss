@import '/css/aui.wxss';
@import '/css/common.wxss';
@import '/css/global.wxss';
.active{color: #ff4805 !important;}

.aui-list-item-media {
  width: auto !important;
}

.shop-pic {
  width: 160rpx;
  height: 160rpx;
  border-radius: 10rpx;

  overflow: hidden;
}

.shop-pic image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.aui-btn {
  margin-left: 20rpx;
  height: 50rpx !important;
  line-height: 50rpx !important;
  /* width: 3.5rem; */
}

.aui-btn-outlined {
  border-color: #ff4805 !important;
  color: #ff4805 !important;
}