@import '/css/aui.wxss';
@import '/css/common.wxss';
page{ background-color: rgba(0,0,0,0.2)}
.aui-content{ width: 70%;height: 100%;position: fixed;top: 0;background-color: #fff;z-index: 10000}
.head{ width: 100rpx;height: 100rpx;border-radius: 50%;object-fit: cover}
.aui-list-item-title{ display: flex;align-items: center;}
.aui-list{ margin-top:88rpx}
.icon{ width: 40rpx;height: 40rpx;}
.tag{ height: 35rpx;padding: 0 12rpx;color: #fff;border-radius: 35rpx;border-top-left-radius: 0 !important;}
.hide{ width: 100%;height: 100%;z-index: 1000;position: fixed;top: 0;background-color: rgba(0,0,0,0)}
button {
  background: none;
  border: 1px solid rgba(0,0,0,0) !important;
  width:120rpx !important;
  padding: 0 !important;
  line-height: auto !important;
  height: auto !important;
  position: relative;
  top:10rpx;


}
button:after {
  background: none;
  border: 1px solid rgba(0,0,0,0) !important;

}